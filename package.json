{
	"name": "wordpress-hook-navigator",
	"displayName": "Wordpress Hook Navigator",
	"description": "Find function & tag definition of the wordpress hook",
	"version": "0.0.2",
	"publisher": "nogomin",
	"icon": "images/wordpress-hook-navigator-logo.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nogomin/wordpress-hook-navigator.git"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"wordpress",
		"hook",
		"navigator",
		"definition"
	],
	"activationEvents": [
		"onCommand:extension.goToHookDefinition"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.goToHookDefinition",
				"title": "Hook Navigator"
			}
		],
		"keybindings": [
			{
					"command": "extension.goToHookDefinition",
					"key": "ctrl+a",
					"mac": "cmd+a",
					"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
					{
						"command": "extension.goToHookDefinition",
						"group": "navigation"
					}
				]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.34.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
